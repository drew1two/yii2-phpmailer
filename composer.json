{
    "name": "dicr/yii2-phpmailer",
    "type": "yii2-extension",
    "license": "MIT",
    "homepage": "https://gitlab.com/dicr/yii2-phpmailer",
    "description": "Адаптер PHPMailer для Yii2",
    "keywords": ["dicr", "yii2", "phpmailer", "mail", "adaper"],
    "authors": [
        {
            "name": "Igor Tarasov",
            "email": "develop@dicr.org",
            "homepage": "http://dicr.org",
            "role": "developer"
        }
    ],
    "support": {
        "source": "https://gitlab.com/dicr/yii2-phpmailer.git"
    },
    "autoload": {
        "psr-4": {
            "dicr\\phpmailer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "dicr\\tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "~7.4 || ~8.0.1",

        "yiisoft/yii2": "~2.0",

        "phpmailer/phpmailer": "~v6.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "~9.5"
    },
    "repositories": [
        {"type": "composer", "url": "https://asset-packagist.org"}
    ]
}
